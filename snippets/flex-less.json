{
	"Flex mixins": {
    "prefix": "flx-less",
    "body": [
      ".flex() {",
      "  display: flex;",
      "}",
      ".flex-wrap() {",
      "  flex-wrap: wrap;",
      "}",
      ".flex-column() {",
      "  flex-direction: column;",
      "}",
      ".flex-start() {",
      "  justify-content: flex-start;",
      "}",
      ".flex-center() {",
      "  justify-content: center;",
      "}",
      ".flex-end() {",
      "  justify-content: flex-end;",
      "}",
      ".flex-top() {",
      "  align-items: flex-start;",
      "}",
      ".flex-middle() {",
      "  align-items: center;",
      "  align-content: center;",
      "}",
      ".flex-bottom() {",
      "  align-items: flex-end;",
      "  align-content: flex-end;",
      "}",
      ".flex-self() {",
      "  align-self: center;",
      "}",
      ".flex-between() {",
      "  justify-content: space-between;",
      "}",
      "",
      ".flex-0() {",
      "  flex-shrink: 0;",
      "}",
      ".flex-1() {",
      "  flex: 1;",
      "  flex-basis: 0%;",
      "  width: 0;",
      "}",
      "",
      ".flex-normal-center(){",
      "  display: flex;",
      "  justify-content: center;",
      "  align-items: center;",
      "}",
      ""
    ],
    "description": "flex class in mixin format"
  }
}